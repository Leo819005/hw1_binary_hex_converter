#include <stdio.h>

int main() {
    int input;
    scanf("%d", &input);
    
    int binary[32];
    int index = 0;
    
    if (input == 0) {
        printf("0");
        return 0;
    }
    
    while (input > 0) {
        binary[index] = input % 2;
        input = input / 2;
        index++;
    }
    
    for (int i = index - 1; i >= 0; i--) {
        printf("%d", binary[i]);
    }
    
    return 0;
}

